#include "../MCAL/gpio.h"
#include "../MCAL/adc.h"
#include "../HAL/lcd.h"
#include "../HAL/lm35_sensor.h"
#include "../HAL/DC_Motor.h"
#include "../MCAL/PWM0.h"


void main(void)
{
	//initialize the ADC driver.
	ADC_ConfigType adc_config = {Internal_2_56V,F_CPU_8};
	ADC_init(&adc_config);

	//Initialize the LCD driver.
	LCD_init();

	//Initialize the DC Motor driver.
	DcMotor_Init();


}
